{
  "name": "anthropic-dark",
  "settings": [
    {
      "settings": {
        "background": "#141413",
        "foreground": "#E8E3DC",
        "caret": "#E8E3DC",
        "selection": "#E89A7A33",
        "selectionForeground": "#E8E3DC",
        "lineHighlight": "#2A251E80"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#8A7F73",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.delete",
        "keyword.other.using",
        "keyword.other.operator"
      ],
      "settings": {
        "foreground": "#F2A784",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call.generic",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#E89A7A",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter"
      ],
      "settings": {
        "foreground": "#E89A7A"
      }
    },
    {
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#D4B896"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#C7A47E",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "entity.name.variable",
        "variable.other.constant",
        "variable.other.object",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#B5A18B"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "punctuation",
        "meta.brace",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#9C8F82"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#F2A784"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.inline",
        "meta.tag.inline entity.other.attribute-name",
        "meta.tag.block entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#B5A18B"
      }
    },
    {
      "scope": [
        "support.type.builtin",
        "support.type.primitive",
        "entity.name.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#E89A7A",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "meta.preprocessor",
        "entity.name.function.preprocessor",
        "meta.preprocessor.string",
        "meta.preprocessor.numeric",
        "meta.structure.dictionary.key.python"
      ],
      "settings": {
        "foreground": "#7A6F63"
      }
    },
    {
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#C9A97D",
        "background": "#2A251E"
      }
    },
    {
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#C7A47E",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "foreground": "#E89A7A",
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "foreground": "#F2A784",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#141413",
        "background": "#F2A784"
      }
    }
  ]
}