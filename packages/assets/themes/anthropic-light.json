{
  "name": "anthropic-light",
  "settings": [
    {
      "settings": {
        "background": "#F9F9F7",
        "foreground": "#2B2A27",
        "caret": "#2B2A27",
        "selection": "#D9775733",
        "selectionForeground": "#2B2A27",
        "lineHighlight": "#F0EEE680"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#A09B93",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.delete",
        "keyword.other.using",
        "keyword.other.operator"
      ],
      "settings": {
        "foreground": "#C8623F",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call.generic",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#D97757",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter"
      ],
      "settings": {
        "foreground": "#D97757"
      }
    },
    {
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#B8956A"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#A67C52",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "entity.name.variable",
        "variable.other.constant",
        "variable.other.object",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#6B5D4F"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "punctuation",
        "meta.brace",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#8B7F72"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#C8623F"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.inline",
        "meta.tag.inline entity.other.attribute-name",
        "meta.tag.block entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#6B5D4F"
      }
    },
    {
      "scope": [
        "support.type.builtin",
        "support.type.primitive",
        "entity.name.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#D97757",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "meta.preprocessor",
        "entity.name.function.preprocessor",
        "meta.preprocessor.string",
        "meta.preprocessor.numeric",
        "meta.structure.dictionary.key.python"
      ],
      "settings": {
        "foreground": "#959088"
      }
    },
    {
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#9D7A56",
        "background": "#F4F1EA"
      }
    },
    {
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#A67C52",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "foreground": "#D97757",
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "foreground": "#C8623F",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#F9F9F7",
        "background": "#C8623F"
      }
    }
  ]
}